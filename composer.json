{
    "name" : "bretrzaun/statuspage",
    "description": "Adds a status page to a Silex app",
    "license": "MIT",
    "authors" : [
        {
            "name" : "Bret R. Zaun",
            "email" : "bretrzaun@hotmail.com"
        }
    ],
    "autoload" : {
        "psr-4" : {
            "BretRZaun\\StatusPage\\" : "src/"
        }
    },
    "require": {
        "silex/silex": "^2.0",
        "twig/twig": "^1.23",
        "guzzlehttp/guzzle": "^6.0"
    },
    "require-dev" : {
        "phpunit/phpunit" : "~5.0",
        "symfony/browser-kit": "^2.7",
        "symfony/css-selector": "^2.7",
        "doctrine/dbal": "~2.2"
    },
    "suggest": {
        "doctrine/dbal" : "for checking DB connections"
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "1.1.x-dev"
        }
    }
}
